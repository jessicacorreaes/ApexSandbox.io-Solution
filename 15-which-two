public String whichTwo(Integer a, Integer b, Integer c) {
    String result = '';
    if((a+b) == c){
        result = 'c';
        return result;
    }
    if((a+c) == b){
        result = 'b';
        return result;
    }
    if(b+c == a){
        result = 'a';
        return result;
    }
    return result;

}