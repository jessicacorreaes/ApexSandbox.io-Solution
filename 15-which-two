public String whichTwo(Integer a, Integer b, Integer c) {
    String result = ' ';
    if((a+b) == c){
        result = String.valueOf(c);
        return result;
    }
    if((a+c) == b){
        result = String.valueOf(b);
        return result;
    }
    if(b+c == a){
        result = String.valueOf(a);
        return result;
    }
    return result;

}