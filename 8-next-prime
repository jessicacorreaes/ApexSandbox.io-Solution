public Integer nextPrime(Integer num) {
if(num == 1){
    return 2;
}
    for(Integer i = 2; i< num; i++){
       if( Math.mod(num, i) != 0){
           return num;
       }
    }

    for(Integer i = num; i == num; num++){
       if( Math.mod(num, i) == 0){
           return i;
    }   else if(Math.mod(num, i) != 0){
        num += 1;
        return Math.mod(num,i);
    }
    }
}



    return true;
}

